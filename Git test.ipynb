   "metadata": {
    "collapsed": false
   },
   "outputs": [],
   "source": [
    "funs = [\n",
    "    lambda x: \"Fizz\" if not (x % 3) else None,\n",
    "    lambda x: \"Buzz\" if not (x % 5) else None\n",
    "]\n",
    "\n",
    "ans = [ f(i) for i in range(100) for f in funs ]\n",
    "ans = filter(lambda x: x is not None, ans)"
